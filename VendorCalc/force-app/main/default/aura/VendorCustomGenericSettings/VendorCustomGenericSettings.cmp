<aura:component description="VendorCustomGenericSettings" controller="VendorCustomSettings" access="global" implements="force:appHostable">
    <ltng:require styles="{!$Resource.ANZ + '/ANZ/css/jquery-ui.min.accordion.css'}"
                  scripts="{!join(',',
                          $Resource.ANZ + '/ANZ/js/jquery.min.js',
                          $Resource.ANZ + '/ANZ/js/jquery.session.js',
                          $Resource.ANZ + '/ANZ/js/jquery-ui.min.js'
                          )}" afterScriptsLoaded="{!c.doInit}"/>
    <aura:attribute name="showOptionButton" type="Boolean" default="false"/>
    <aura:attribute name="loanFacilityType" type="String" default=""/>
    <aura:attribute name="termSelected" type="Integer" default=""/>
    <aura:attribute name="showExistingSettings" type="Boolean" default="false"/>
    <aura:attribute name="allExistingPriceSettings" type="Object[]" default=""/>
    <aura:attribute name="listOptionsGoodsType" type="List" default="[]"/>
    <aura:attribute name="PicklistOptionsGoodsType" type="List" default="[]"/>
    <aura:attribute name="selectOptionsGoodsType" type="List" default="[]"/>
    <aura:attribute name="existingAssetsGoodsType" type="List" default="[]"/>
    <aura:attribute name="showSpinner" type="Boolean" default="false"/>
    <aura:attribute name="variant" type="String"/>
    <aura:attribute name="message" type="String"/>
    <aura:attribute name="ProgramPartner" type="String"/>
    <aura:attribute name="ExistingProgramPartner" type="String"/>
    <aura:attribute name="ExistingLoanFacilityType" type="String"/>
    <aura:attribute name="showToast" type="Boolean" default="false"/>
    <aura:attribute name="isNewProgramError" type="Boolean" default="false"/>
    <aura:attribute name="openCustomSettingSection" type="Boolean" default="false"/>
    <aura:attribute name="openSettingSection" type="Boolean" default=""/>
    <aura:attribute name="alertOnSaveSettings" type="Boolean" default="false"/>
    <aura:attribute name="alertOnCopyPricingSetting" type="Boolean" default="false"/>
    <aura:attribute name="openCopyPricingSection" type="Boolean" default="false"/>
    <aura:attribute name="openProgramPricingSection" type="Boolean" default="false"/>
    <aura:attribute name="openAssetsSection" type="Boolean" default="false"/>
    <aura:attribute name="copyProgramPricingSection" type="Boolean" default="false"/>
    <aura:attribute name="copyAssetPricingSection" type="Boolean" default="false"/>
    <aura:attribute name="noSettingsExist" type="Boolean" default="false"/>
    <aura:attribute name="nerSettingsProgramPartner" type="String" default=""/>
    <aura:handler name="change" value="{!v.nerSettingsProgramPartner}" action="{!c.setGoodsTypeAction}"/>
    <aura:attribute name="selectedCopyPriceSettings" type="Object"
                    description="Hold all the values for copy Existing setting"/>
    <aura:attribute name="calcSettings" type="Calculator_Settings__c"
                    default="{sobjectType:'Calculator_Settings__c'}"/>

    <aura:handler name="change" value="{!v.loanFacilityType}" action="{!c.changeLoanFacilityType}" /> 
    <aura:handler name="change" value="{!v.ProgramPartner}" action="{!c.changeProgramPartner}" />
    <lightning:spinner class="{!v.showSpinner ? 'slds-show' : 'slds-hide'}" variant="brand" alternativeText="Please wait..."/>

    <aura:if isTrue="{!v.showToast}">
        <c:Toast variant="{!v.variant}" message="{!v.message}"/>
    </aura:if>

    <form >
        <fieldset class="myFieldset">
            <legend class="myLegend">Program Pricing</legend>
            <aura:if isTrue="{!!v.openCustomSettingSection}">
                <div class="slds-grid slds-grid_align-spread" >
                    <div class="slds-col slds-p-around_small">
                        <!--<lightning:button iconName="utility:back" label="Back"
                                          onclick="{!c.closeOptionSettings}"/>-->
                    </div>
                    <div class="slds-col slds-p-around_small slds-grid slds-grid_align-spread">
                        <div class="slds-col slds-p-horizontal_small">
                            <lightning:select name="termSelectedGeneric" value="{!v.termSelected}"
                                              label="" class="inputTerm" onchange="{!c.getSelectedPriceSettings}">
                                <option value="">--Select Term--</option>
                                <option value="24">24</option>
                                <option value="36">36</option>
                                <option value="48">48</option>
                                <option value="60">60</option>
                                <option value="72">72</option>
                                <option value="84">84</option>
                            </lightning:select>
                        </div>
                        <div class="slds-col">
                            <aura:if isTrue="{!and(v.termSelected != '', v.noSettingsExist == false)}">
                                <lightning:button iconName="utility:copy" label="Copy Pricing"
                                                  onclick="{!c.openCopyPricingSettings}"/>
                            </aura:if>
                            <lightning:button iconName="utility:add" label="New"
                                              onclick="{!c.newProgramPricing}"/>
                        </div>
                    </div>
                </div>
                <aura:if isTrue="{!and(v.termSelected != '', v.showExistingSettings)}">
                    <h1 class="slds-p-around_xx-small head slds-m-top_medium">
                        Program Pricing
                    </h1>
                    <div class="slds-box">
                        <div id="accordionProgram" class="accordion">
                            <aura:if isTrue="{!v.allExistingPriceSettings.length > 0}">
                                <aura:iteration items="{!v.allExistingPriceSettings}"
                                                var="priceSettingObj" indexVar="index">
                                    <aura:if isTrue="{!or(priceSettingObj.VEN3__Program_Partner__c != null, priceSettingObj.VEN3__Program_Partner__c != undefined)}">
                                        <h3>{!priceSettingObj.VEN3__Program_Partner__r.Name} - {!priceSettingObj.VEN3__Goods_Type__c} - {!priceSettingObj.VEN3__Asset_Type__c}</h3>
                                        <div>
                                            <div class="slds-grid slds-wrap">
                                                <div class="slds-col slds-size_1-of-1 slds-medium-size_3-of-6 slds-large-size_4-of-5 slds-align--absolute-center"></div>
                                                <div class="slds-col slds-size_1-of-1 slds-medium-size_3-of-6 slds-large-size_1-of-5 slds-align--absolute-center slds-p-around--xx-small">
                                                    <aura:if isTrue="{!priceSettingObj.isEdit}">
                                                        <lightning:buttonIcon alternativeText="Save"
                                                                              value="{!index}"
                                                                              iconName="utility:save"
                                                                              onclick="{!c.saveEditSetting}"/>
                                                        <aura:set attribute="else">
                                                        <lightning:buttonIcon alternativeText="Edit"
                                                                              value="{!index}"
                                                                              iconName="utility:edit"
                                                                              onclick="{!c.editPriceSetting}"/>
                                                        </aura:set>
                                                    </aura:if>
                                                    <lightning:buttonIcon alternativeText="Delete"
                                                                          value="{!index}"
                                                                          iconName="utility:delete"
                                                                          onclick="{!c.deletePriceSetting}"/>
                                                </div>
                                                <div class="slds-col slds-size_1-of-1 slds-medium-size_3-of-6 slds-large-size_2-of-5 slds-p-left--small">
                                                    <ui:inputCurrency aura:id=""
                                                                      value="{!priceSettingObj.VEN3__Establishment_Fee__c}"
                                                                      label="Establishment Fee"
                                                                      labelClass="slds-form-element__label"
                                                                      placeholder="$" class="slds-input"
                                                                      disabled="{!!priceSettingObj.isEdit}"></ui:inputCurrency>
                                                </div>
                                                <div class="slds-col slds-size_1-of-1 slds-medium-size_3-of-6 slds-large-size_1-of-5"></div>
                                                <div class="slds-col slds-size_1-of-1 slds-medium-size_3-of-6 slds-large-size_2-of-5 slds-p-right--small">
                                                    <ui:inputCurrency aura:id=""
                                                                      value="{!priceSettingObj.VEN3__PPSR_Fee__c}"
                                                                      label="PPSR Fee"
                                                                      labelClass="slds-form-element__label"
                                                                      placeholder="$" class="slds-input"
                                                                      disabled="{!!priceSettingObj.isEdit}"></ui:inputCurrency>
                                                </div>
                                                <aura:if isTrue="{!v.loanFacilityType == 'Rental - Apply To Buy'}">
                                                    <div class="slds-col slds-size_1-of-1 slds-medium-size_3-of-6 slds-large-size_2-of-5 slds-p-left--small">
                                                        <ui:inputNumber aura:id=""
                                                                        value="{!priceSettingObj.VEN3__Payments_to_Own__c}"
                                                                        label="Payments to Own (Apply to Buy)"
                                                                        labelClass="slds-form-element__label"
                                                                        class="slds-input"
                                                                        disabled="{!!priceSettingObj.isEdit}"></ui:inputNumber>
                                                    </div>
                                                </aura:if>
                                                <div class="slds-col slds-size_1-of-1 slds-medium-size_3-of-6 slds-large-size_3-of-5 slds-p-left--small"></div>
                                                <div class="slds-col slds-size_1-of-1 slds-medium-size_3-of-6 slds-large-size_5-of-5 slds-p-around--small">
                                                    <aura:if isTrue="{!!priceSettingObj.isEdit}">
                                                        <table border="1"
                                                               class="slds-table slds-table_striped tableLayoutEdit" style="width: 100%">
                                                            <thead>
                                                            <tr class="slds-text-title_caps">
                                                                <th width="16%" scope="col" class="thtdwrap">
                                                                    <div class="slds-align--absolute-center"
                                                                         title="Cost of Goods Inc. GST">
                                                                        Cost of Goods Inc. GST
                                                                    </div>
                                                                </th>
                                                                <th width="12%" scope="col" class="thtdwrap">
                                                                    <div class="slds-align--absolute-center"
                                                                         title="Funder Name">Funder Name
                                                                    </div>
                                                                </th>
                                                                <th width="9%" scope="col" class="thtdwrap">
                                                                    <div class="slds-align--absolute-center"
                                                                         title="Base Rate">Base Rate
                                                                    </div>
                                                                </th>
                                                                <th width="9%" scope="col" class="thtdwrap">
                                                                    <div class="slds-align--absolute-center"
                                                                         title="Chase Brokerage">Brokerage
                                                                    </div>
                                                                </th>
                                                                <th width="9%" scope="col" class="thtdwrap">
                                                                    <div class="slds-align--absolute-center"
                                                                         title="Balloon">Balloon(0)
                                                                    </div>
                                                                </th>
                                                                <th width="9%" scope="col" class="thtdwrap">
                                                                    <div class="slds-align--absolute-center"
                                                                         title="Balloon">Balloon(10)
                                                                    </div>
                                                                </th>
                                                                <th width="9%" scope="col" class="thtdwrap">
                                                                    <div class="slds-align--absolute-center"
